<div class="row clearfix">
    <% console.log(urlPrefix) %>
    <% console.log(url) %>
    <% var page = Math.ceil(count / limit); %>
    <% var current=x; %>
    <div class="col-md-12"><br /></div>

    <div class="col-md-6 text-center">
        <span>Displaying
            <% if(count===0){ %>
            0
            <% }else{ %>
            <%= ((current-1)*limit)+1 %>
            <% } %>
            -
            <% if(((current-1)*limit+limit)>count){ %>
            <%=count%>
            <% }else{ %>
            <%= (current-1)*limit+limit %>
            <% } %> of <%=count%>
            records</span>
    </div>

    <div class="col-md-6 text-center">
        <span>Page <input type="number" class="pageNo" value="<%=x%>" step="1" min="1" max="<%=page%>"> of
            <%= page %></span>

        <div class="btn-group m-btn-group" role="group" aria-label="...">
            <button type="button" data-url="<%=url%>" data-pageLimit="<%=page%>" onclick="goToPage(this)"
                class="btn btn-secondary">GO</button>
        </div>
        <div class="btn-group m-btn-group" role="group" aria-label="...">
            <% if(x > 1) { %>
            <a href="/<%=url%>?page=<%=x-1%><%=urlPrefix%>"><button type="button"
                    class="btn btn-secondary">Previous</button></a>
            <% } %>
            <% if(x < page ) { x++; %>
            <a href="/<%=url%>?page=<%= x %><%=urlPrefix%>"><button type="button"
                    class="btn btn-secondary">Next</button></a>
            <% } %>
        </div>
    </div>
</div>